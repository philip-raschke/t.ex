{
  "manifest_version": 2,
  "name": "T.EX - The Transparency EXtension",
  "description": "Visualize third-party communication with T.EX to identify Web trackers and what they know about you.",
  "version": "0.9.4",
  "icons": {
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "T.EX - Transparency EXtension",
    "default_popup": "pages/popup_page/index.html"
  },
  "permissions": [
    "downloads",
    "storage",
    "unlimitedStorage"
  ]
}
